{
  "name": "hello-world-containerized",
  "version": "1.0.0",
  "main": "app/backend/server.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "prod_cont": "cross-env NODE_ENV=production ./startup_scripts/run-docker-bash.sh",
    "prod_cont_ps": "cross-env NODE_ENV=production powershell -ExecutionPolicy Bypass -File ./startup_scripts/run-docker-powershell.ps1",
    "dev_cont": "cross-env NODE_ENV=container ./startup_scripts/run-docker-bash.sh",
    "dev_cont_ps": "cross-env NODE_ENV=container powershell -ExecutionPolicy Bypass -File ./startup_scripts/run-docker-powershell.ps1",
    "local_dev": "cross-env NODE_ENV=development ./startup_scripts/run-local-bash.sh",
    "local_dev_ps": "cross-env NODE_ENV=development powershell -ExecutionPolicy Bypass -File ./startup_scripts/run-local-powershell.ps1",
    "start": "node --env-file .env app/backend/server.js -L -e js,mjs,html,ejs,css,toml",
    "stop": "docker-compose down",
    "dev": "nodemon --env-file .env app/backend/server.js -L -e js,mjs,html,ejs,css,toml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "mongoose": "^8.8.1",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "pino-pretty": "^11.2.2",
    "serve-favicon": "^2.5.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "open": "^10.1.0",
    "sass": "^1.80.7"
  }
}
